.boxes {
  height: 100%;
  background-color: #fff;
  max-width: 884px;
  width: 100%;
  margin: auto;
}
.big-carousel_wrap {
  @media screen and (width <= 992px) {
    display: block;
    margin: auto;
    max-width: 458px;
    width: 100%;
  }
}
.slick-dots {
  padding: 0;
  list-style: none;
  display: block;
  width: 122px;
  margin: auto;
  margin-top: 52px;
  padding-bottom: 40px;

  & li {
    float: left;
    padding-left: 37px;

    @media screen and (width <= 992px) {
      padding-left: 5px;
    }

    &:first-child {
      padding-left: 0;
    }
  }

  & li button {
    width: 16px;
    height: 16px;
    background-color: #d8d8d8;
    border-radius: 50%;
    font-size: 0;
    border: none;
    cursor: pointer;
  }

  & li.slick-active button{
    background-color: var(--main);
  }
}

